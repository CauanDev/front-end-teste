{"ast":null,"code":"import { defineComponent } from 'vue';\nimport Chart from 'chart.js/auto';\nexport default defineComponent({\n  name: 'SexChard',\n  props: {\n    users: {\n      type: Array\n    }\n  },\n  data() {\n    return {\n      chart: null\n    };\n  },\n  watch: {\n    users() {\n      if (this.chart) {\n        this.chart.destroy();\n      }\n      this.renderChart();\n    }\n  },\n  methods: {\n    renderChart() {\n      const maleCount = this.users.filter(user => user.implementar.sex === 'M').length;\n      const femaleCount = this.users.filter(user => user.implementar.sex === 'F').length;\n      const ctx = this.$refs.sexChart.getContext('2d');\n      if (this.chart) {\n        this.chart.destroy();\n      }\n      this.chart = new Chart(ctx, {\n        type: 'bar',\n        data: {\n          labels: ['Masculino', 'Feminino'],\n          datasets: [{\n            label: 'Número de Usuários por Sexo',\n            data: [maleCount, femaleCount],\n            backgroundColor: ['#007bff', '#dc3545'],\n            borderWidth: 1\n          }]\n        },\n        options: {\n          responsive: true,\n          scales: {\n            y: {\n              beginAtZero: true\n            }\n          }\n        }\n      });\n    }\n  },\n  mounted() {\n    this.renderChart();\n  }\n});","map":{"version":3,"names":["defineComponent","Chart","name","props","users","type","Array","data","chart","watch","destroy","renderChart","methods","maleCount","filter","user","implementar","sex","length","femaleCount","ctx","$refs","sexChart","getContext","labels","datasets","label","backgroundColor","borderWidth","options","responsive","scales","y","beginAtZero","mounted"],"sources":["/home/kel/Documents/PHP/Projetos/TesteJunior/front-end-teste/src/components/chards/SexChard.vue"],"sourcesContent":["<template>\n  <div>\n    <canvas ref=\"sexChart\"></canvas>\n  </div>\n</template>\n\n<script>\nimport { defineComponent } from 'vue';\nimport Chart from 'chart.js/auto';\n\nexport default defineComponent({\n  name: 'SexChard',\n  props: {\n    users: {\n      type: Array,\n    },\n  },\n  data() {\n    return {\n      chart: null,\n    };\n  },\n\n  watch: {\n    users() {\n      if (this.chart) {\n        this.chart.destroy();\n      }\n      this.renderChart();\n    },\n  },\n  methods: {\n    renderChart() {\n      const maleCount = this.users.filter(user => user.implementar.sex === 'M').length;\n      const femaleCount = this.users.filter(user => user.implementar.sex === 'F').length;\n      const ctx = this.$refs.sexChart.getContext('2d');\n      if (this.chart) {\n        this.chart.destroy();\n      }\n      this.chart = new Chart(ctx, {\n        type: 'bar',\n        data: {\n          labels: ['Masculino', 'Feminino'],\n          datasets: [{\n            label: 'Número de Usuários por Sexo',\n            data: [maleCount, femaleCount],\n            backgroundColor: ['#007bff', '#dc3545'],\n            borderWidth: 1,\n          }],\n        },\n        options: {\n          responsive: true,\n          scales: {\n            y: {\n              beginAtZero: true,\n            },\n          },\n        },\n      });\n    },\n  },\n  mounted() {\n    this.renderChart();\n  },\n});\n</script>\n"],"mappings":"AAOA,SAASA,eAAc,QAAS,KAAK;AACrC,OAAOC,KAAI,MAAO,eAAe;AAEjC,eAAeD,eAAe,CAAC;EAC7BE,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE;IACLC,KAAK,EAAE;MACLC,IAAI,EAAEC;IACR;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EAEDC,KAAK,EAAE;IACLL,KAAKA,CAAA,EAAG;MACN,IAAI,IAAI,CAACI,KAAK,EAAE;QACd,IAAI,CAACA,KAAK,CAACE,OAAO,CAAC,CAAC;MACtB;MACA,IAAI,CAACC,WAAW,CAAC,CAAC;IACpB;EACF,CAAC;EACDC,OAAO,EAAE;IACPD,WAAWA,CAAA,EAAG;MACZ,MAAME,SAAQ,GAAI,IAAI,CAACT,KAAK,CAACU,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACC,WAAW,CAACC,GAAE,KAAM,GAAG,CAAC,CAACC,MAAM;MAChF,MAAMC,WAAU,GAAI,IAAI,CAACf,KAAK,CAACU,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACC,WAAW,CAACC,GAAE,KAAM,GAAG,CAAC,CAACC,MAAM;MAClF,MAAME,GAAE,GAAI,IAAI,CAACC,KAAK,CAACC,QAAQ,CAACC,UAAU,CAA