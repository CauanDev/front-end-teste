{"ast":null,"code":"import TableView from \"@/components/Table/TableView.vue\";\nimport http from '@/services/http.js';\nimport TitleView from \"../components/Title/TitleView.vue\";\nexport default {\n  name: \"DashBoard\",\n  components: {\n    TableView,\n    TitleView\n  },\n  data() {\n    return {\n      results: [],\n      openModal: true,\n      modalContent: {},\n      newResults: {\n        name: '',\n        values: ''\n      }\n    };\n  },\n  methods: {\n    async getNameTempalte(id) {\n      return await http.get('/templates/' + id);\n    },\n    async getResults() {\n      try {\n        const response = await http.get('/results');\n        console.log(response.data.templates.data);\n        console.log(this.getNameTempalte(1));\n        this.results = await Promise.all(response.data.templates.data.map(async item => {\n          const nomeDiagnostico = await this.getNameTemplate(item.id);\n          return {\n            nomeLoterica: item.name,\n            values: item.values,\n            tipoDiagnostico: nomeDiagnostico,\n            implementar: {\n              nomeLoterica: item.nomeLoterica,\n              values: item.values\n            }\n          };\n        }));\n      } catch (error) {\n        console.log(error.response);\n      }\n    },\n    showDetails(details) {\n      this.modalContent = details;\n      this.newResults = details;\n      this.openModal = false;\n    }\n  },\n  mounted() {\n    this.getResults();\n  }\n};","map":{"version":3,"names":["TableView","http","TitleView","name","components","data","results","openModal","modalContent","newResults","values","methods","getNameTempalte","id","get","getResults","response","console","log","templates","Promise","all","map","item","nomeDiagnostico","getNameTemplate","nomeLoterica","tipoDiagnostico","implementar","error","showDetails","details","mounted"],"sources":["C:\\Users\\Usuário\\Documents\\projeto_diagnostico\\client\\src\\views\\DashBoard.vue"],"sourcesContent":["<template>