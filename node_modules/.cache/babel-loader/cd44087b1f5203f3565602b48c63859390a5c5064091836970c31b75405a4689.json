js\"\r\n\r\nimport http from '@/services/http.js';\r\n\r\n  export default {\r\n    name:\"CreateResults\",\r\n    data() {\r\n      return {\r\n        salarioOperador:[\"R$1.400\",\"R$1.500\",\"R$1.600\",\"R$1.700\",\"R$1.800\"],\r\n        salarioGerente:[\"R$1.600\",\"R$1.800\",\"R$2.000\",\"R$2.500\",\"R$3.000\"],\r\n        salarioLoterico:[\"R$2.500\",\"R$5.000\",\"R$7.500\",\"R$10.000\",\"R$15.000\",\"R$16.000+\"],\r\n        types:[],\r\n        user:{\r\n        email:'',\r\n        password: ''},\r\n        isPasswordValid: true,\r\n        isEmailValid: true,\r\n        isFormValid: false\r\n      };\r\n    },\r\n    methods: {\r\n      async submitForm() {\r\n        this.isFormValid = (this.isPasswordValid  && this.isEmailValid) \r\n        if(this.isFormValid){\r\n                \r\n          try {\r\n              const auth = useAuth();\r\n              const {data} = await http.post('/login',this.user);\r\n              auth.setToken(data.token);\r\n              auth.setUser(data.user);\r\n              this.$router.push({ name: \"Dashboard\" });\r\n          } catch (error) {\r\n            console.log(\"deu errado \"+error)\r\n          }\r\n                   \r\n        }\r\n      },\r\n      validateEmail() \r\n      {\r\n        const emailPattern = /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/;\r\n        this.isEmailValid = !emailPattern.test(this.user.email)? false:true;\r\n      },\r\n      validatePassword() \r\n      {\r\n        this.isPasswordValid=  this.user.password.length < 6 ? false : true\r\n      },\r\n      getResults(){\r\n        try {\r\n                  const results = http.get(\"/template\");\r\n                  console.log(results.data.templates.data)\r\n        } catch (error) {\r\n          console.log(error)\r\n        }\r\n\r\n      }\r\n\r\n    },\r\n    mounted() {\r\n    this.getResults();\r\n  }\r\n  };\r\n  </script>\r\n\r\n  "],"mappings":";AAuHA,SAAQA,OAAO,QAAO,kBAAiB;AAEvC,OAAOC,IAAG,MAAO,oBAAoB;AAEnC,eAAe;EACbC,IAAI,