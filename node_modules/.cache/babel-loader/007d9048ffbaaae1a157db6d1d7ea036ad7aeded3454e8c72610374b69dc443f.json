{"ast":null,"code":"import http from '@/services/http.js';\nimport TitleView from '../components/Title/TitleView.vue';\nimport TableView from '../components/Table/TableView.vue';\nimport { format } from 'date-fns';\nexport default {\n  name: 'UpdateUser',\n  components: {\n    TitleView,\n    TableView\n  },\n  data() {\n    return {\n      users: [],\n      openModal: false,\n      modalContent: ''\n    };\n  },\n  methods: {\n    async getUsers() {\n      try {\n        const data = await http.get('/users');\n        console.log(data.data.users);\n        this.users = data.data.users.map(user => {\n          return {\n            name: user.name,\n            email: user.email,\n            created_at: format(new Date(user.created_at), 'dd/MM/yyyy'),\n            implementar: {\n              name: user.name,\n              email: user.email\n            }\n          };\n        });\n      } catch (error) {\n        console.log(error.response);\n      }\n    },\n    showDetails(details) {\n      this.modalContent = details;\n      this.openModal = false;\n    },\n    closeModal() {\n      this.showModal = false;\n      this.modalContent = '';\n    }\n  },\n  mounted() {\n    this.getUsers();\n  }\n};","map":{"version":3,"names":["http","TitleView","TableView","format","name","components","data","users","openModal","modalContent","methods","getUsers","get","console","log","map","user","email","created_at","Date","implementar","error","response","showDetails","details","closeModal","showModal","mounted"],"sources":["C:\\Users\\Usuário\\Documents\\projeto_diagnostico\\client\\src\\views\\UpdateUser.vue"],"sourcesContent":["<template>\r\n  <div>\r\n  <TitleView title=\"Atualizar Usuários\"/>\r\n  </div>\r\n  <div class=\" flex justify-center mt-10\">\r\n     <TableView :headers=\"['Nome','Email','Criado Em','Ver Detalhes']\" :body=\"users\" @showDetails=\"showDetails\" class=\"w-[50%]\"/>  \r\n  </div>\r\n\r\n  <div id=\"default-modal\" :class=\"{'hidden':openModal}\" class=\" fixed inset-0 flex items-center justify-center bg-black bg-opacity-50\">\r\n    <div class=\"relative p-4 w-full max-w-2xl max-h-full\">\r\n        <!-- Modal content -->\r\n        <div class=\"relative bg-white rounded-lg shadow\">\r\n            <!-- Modal header -->\r\n            <div class=\"flex items-center justify-between p-4 md:p-5 border-b rounded-t\">\r\n                <h3 class=\"text-xl font-semibold text-gray-900 \">\r\n                    Alterar {{modalContent.name}}\r\n                </h3>\r\n                <button type=\"button\" class=\"text-gray-400 bg-transparent hover:bg-gray-200 hover:text-gray-200 rounded-lg text-sm w-8 h-8 ms-auto inline-flex justify-center items-center \" data-modal-hide=\"default-modal\" @click=\"openModal=!openModal\">\r\n                    <svg class=\"w-3 h-3\" aria-hidden=\"true\" xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 14 14\">\r\n                        <path stroke=\"currentColor\" stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"m1 1 6 6m0 0 6 6M7 7l6-6M7 7l-6 6\"/>\r\n                    </svg>\r\n                    <span class=\"sr-only\">Close modal</span>\r\n                </button>\r\n            </div>\r\n            <!-- Modal body -->\r\n            <div class=\"p-4 md:p-5 space-y-4\">\r\n                <p class=\"text-base leading-relaxed text-gray-500 \">\r\n                    With less than a month to go before the European Union enacts new consumer privacy laws for its citizens, companies around the world are updating their terms of service agreements to comply.\r\n                </p>\r\n                <p class=\"text-base leading-relaxed text-gray-500 \">\r\n                    The European Union’s General Data Protection Regulation (G.D.P.R.) goes into effect on May 25 and is meant to ensure a common set of data rights in the European Union. It requires organizations to notify users as soon as possible of high-risk data breaches that could personally affect them.\r\n                </p>\r\n            </div>\r\n            <!-- Modal footer -->\r\n            <div class=\"flex items-center justify-center p-5 border-t border-gray-200 rounded-b\">\r\n              <button type=\"button\" class=\"focus:outline-none text-white bg-green-700 hover:bg-green-800 focus:ring-4 focus:ring-green-300 font-medium rounded-lg text-sm px-5 py-2.5 me-2 mb-2 dark:bg-green-600 dark:hover:bg-green-700 dark:focus:ring-green-800\">Gravar</button>\r\n\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\r\n\r\n\r\n\r\n</template>\r\n\r\n<script>\r\n\r\nimport http from '@/services/http.js';\r\nimport TitleView from '../components/Title/TitleView.vue';\r\nimport TableView from '../components/Table/TableView.vue';\r\nimport { format } from 'date-fns'; \r\n\r\nexport default {\r\n  name: 'UpdateUser',\r\n  components: {\r\n   TitleView,TableView\r\n  },\r\n  data() {\r\n    return {\r\n      users: [] ,\r\n      openModal: false,\r\n      modalContent:''\r\n    };\r\n  },\r\n  methods: {\r\n  async getUsers(){\r\n    try {\r\n      const data = await http.get('/users');\r\n      console.log(data.data.users)\r\n      this.users = data.data.users.map(user => {\r\n          return {\r\n            name: user.name,\r\n            email: user.email,      \r\n            created_at: format(new Date(user.created_at), 'dd/MM/yyyy'),\r\n            implementar: {\r\n              name: user.name,\r\n              email: user.email     \r\n            }\r\n\r\n          };\r\n        }\r\n\r\n      );\r\n\r\n    } catch (error) {\r\n      console.log(error.response)\r\n    }\r\n    },\r\n    showDetails(details) {\r\n      this.modalContent = details;\r\n      this.openModal = false;\r\n    },\r\n    closeModal() {\r\n      this.showModal = false;\r\n      this.modalContent = '';\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getUsers();\r\n  }\r\n};\r\n</script>\r\n<style>\r\n/* Add some basic styling for the modal */\r\n.modal {\r\n  display: block;\r\n  position: fixed;\r\n  z-index: 1;\r\n  left: 0;\r\n  top: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  overflow: auto;\r\n  background-color: rgb(0,0,0);\r\n  background-color: rgba(0,0,0,0.4);\r\n}\r\n\r\n.modal-content {\r\n  background-color: #fefefe;\r\n  margin: 15% auto;\r\n  padding: 20px;\r\n  border: 1px solid #888;\r\n  width: 80%;\r\n}\r\n\r\n.close {\r\n  color: #aaa;\r\n  float: right;\r\n  font-size: 28px;\r\n  font-weight: bold;\r\n}\r\n\r\n.close:hover,\r\n.close:focus {\r\n  color: black;\r\n  text-decoration: none;\r\n  cursor: pointer;\r\n}\r\n</style>"],"mappings":"AAgDA,OAAOA,IAAG,MAAO,oBAAoB;AACrC,OAAOC,SAAQ,MAAO,mCAAmC;AACzD,OAAOC,SAAQ,MAAO,mCAAmC;AACzD,SAASC,MAAK,QAAS,UAAU;AAEjC,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACXJ,SAAS;IAACC;EACX,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAC;MACRC,SAAS,EAAE,KAAK;MAChBC,YAAY,EAAC;IACf,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACT,MAAMC,QAAQA,CAAA,EAAE;MACd,IAAI;QACF,MAAML,IAAG,GAAI,MAAMN,IAAI,CAACY,GAAG,CAAC,QAAQ,CAAC;QACrCC,OAAO,CAACC,GAAG,CAACR,IAAI,CAACA,IAAI,CAACC,KAAK;QAC3B,IAAI,CAACA,KAAI,GAAID,IAAI,CAACA,IAAI,CAACC,KAAK,CAACQ,GAAG,CAACC,IAAG,IAAK;UACrC,OAAO;YACLZ,IAAI,EAAEY,IAAI,CAACZ,IAAI;YACfa,KAAK,EAAED,IAAI,CAACC,KAAK;YACjBC,UAAU,EAAEf,MAAM,CAAC,IAAIgB,IAAI,CAACH,IAAI,CAACE,UAAU,CAAC,EAAE,YAAY,CAAC;YAC3DE,WAAW,EAAE;cACXhB,IAAI,EAAEY,IAAI,CAACZ,IAAI;cACfa,KAAK,EAAED,IAAI,CAACC;YACd;UAEF,CAAC;QACH,CAEF,CAAC;MAEH,EAAE,OAAOI,KAAK,EAAE;QACdR,OAAO,CAACC,GAAG,CAACO,KAAK,CAACC,QAAQ;MAC5B;IACA,CAAC;IACDC,WAAWA,CAACC,OAAO,EAAE;MACnB,IAAI,CAACf,YAAW,GAAIe,OAAO;MAC3B,IAAI,CAAChB,SAAQ,GAAI,KAAK;IACxB,CAAC;IACDiB,UAAUA,CAAA,EAAG;MACX,IAAI,CAACC,SAAQ,GAAI,KAAK;MACtB,IAAI,CAACjB,YAAW,GAAI,EAAE;IACxB;EACF,CAAC;EACDkB,OAAOA,CAAA,EAAG;IACR,IAAI,CAAChB,QAAQ,CAAC,CAAC;EACjB;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}