ngth;\r\n  \r\n          createChart(['Idade Média'], [averageAge], 'Idade Média dos Usuários', ['#007bff']);\r\n        }\r\n      },\r\n    },\r\n  });\r\n  </script>\r\n  "],"mappings":"AAOE,SAASA,eAAc,QAAS,KAAK;AACrC,OAAOC,KAAI,MAAO,eAAe;AAEjC,eAAeD,eAAe,CAAC;EAC7BE,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE;IACLC,KAAK,EAAE;MACLC,IAAI,EAAEC,KAAK;MACXC,QAAQ,EAAE;IACZ,CAAC;IACDC,UAAU,EAAE;MACVH,IAAI,EAAEI,OAAO;MACbF,QAAQ,EAAE;IACZ;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,SAAS,CAAC,MAAM;MACnB,IAAI,CAACC,WAAW,CAAC,CAAC;IACpB,CAAC,CAAC;EACJ,CAAC;EACDC,KAAK,EAAE;IACLX,KAAKA,CAAA,EAAG;MACN,IAAI,CAACY,WAAW,CAAC,CAAC;IACpB,CAAC;IACDR,UAAUA,CAAA,EAAG;MACX,IAAI,CAACQ,WAAW,CAAC,CAAC;IACpB;EACF,CAAC;EACDC,OAAO,EAAE;IACPD,WAAWA,CAAA,EAAG;MACZ,IAAI,IAAI,CAACL,KAAK,EAAE;QACd,IAAI,CAACA,KAAK,CAACO,OAAO,CAAC,CAAC;QACpB,IAAI,CAACP,KAAI,GAAI,IAAI;MACnB;MACA,IAAI,CAACE,SAAS,CAAC,MAAM;QACnB,IAAI,CAACC,WAAW,CAAC,CAAC;MACpB,CAAC,CAAC;IACJ,CAAC;IACDA,WAAWA,CAAA,EAAG;MACZ,MAAMK,GAAE,GAAI,IAAI,CAACC,KAAK,CAACC,QAAQ,EAAEC,UAAU,CAAC,IAAI,CAAC;MACjD,IAAI,CAACH,GAAG,EAAE;MAEV,MAAMI,WAAU,GAAIA,CAACC,MAAM,EAAEd,IAAI,EAAEe,KAAK,EAAEC,gBAAgB,KAAK;QAC7D,IAAI,CAACf,KAAI,GAAI,IAAIV,KAAK,CAACkB,GAAG,EAAE;UAC1Bd,IAAI,EAAE,KAAK;UACXK,IAAI,EAAE;YACJc,MAAM;YACNG,QAAQ,EAAE,CAAC;cACTF,KAAK;cACLf,IAAI;cACJkB,eAAe,EAAEF,gBAAgB;cACjCG,WAAW,EAAE;YACf,CAAC;UACH,CAAC;UACDC,OAAO,EAAE;YACPC,UAAU,EAAE,IAAI;YAChBC,MAAM,EAAE;cACNC,CAAC,EAAE;gBACDC,WAAW,EAAE;cACf;YACF;UACF;QACF,CAAC,CAAC;MACJ,CAAC;MAED,IAAI,IAAI,CAAC1B,UAAU,EAAE;QACnB,MAAM2B,SAAQ,GAAI,IAAI,CAAC/B,KAAK,CAACgC,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACC,WAAW,CAACC,GAAE,KAAM,GAAG,CAAC;QACzE,MAAMC,WAAU,GAAI,IAAI,CAACpC,KAAK,CAACgC,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACC,WAAW,CAACC,GAAE,KAAM,GAAG,CAAC;QAE3E,MAAME,YAAW,GAAIN,SAAS,CAACO,MAAM,CAAC,CAACC,GAAG,EAAEN,IAAI,KAAKM,GAAE,GAAIC,QAAQ,CAACP,IAAI,CAACC,WAAW,CAACO,GAAG,CAAC,EAAE,CAAC,CAAC;QAC7F,MAAMC,cAAa,GAAIN,WAAW,CAACE,MAAM,CAAC,CAACC,GAAG,EAAEN,IAAI,KAAKM,