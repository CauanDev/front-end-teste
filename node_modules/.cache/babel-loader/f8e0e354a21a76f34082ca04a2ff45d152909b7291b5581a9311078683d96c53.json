{"ast":null,"code":"import { defineComponent } from 'vue';\nimport Chart from 'chart.js/auto';\nexport default defineComponent({\n  name: 'AgeChart',\n  props: {\n    users: {\n      type: Array,\n      required: true\n    },\n    splitBySex: {\n      type: Boolean,\n      required: true\n    }\n  },\n  data() {\n    return {\n      chart: null\n    };\n  },\n  mounted() {\n    this.$nextTick(() => {\n      this.renderChart();\n    });\n  },\n  watch: {\n    users() {\n      this.updateChart();\n    },\n    splitBySex() {\n      this.updateChart();\n    }\n  },\n  methods: {\n    updateChart() {\n      if (this.chart) {\n        this.chart.destroy();\n        this.chart = null;\n      }\n      this.$nextTick(() => {\n        this.renderChart();\n      });\n    },\n    renderChart() {\n      const ctx = this.$refs.ageChart?.getContext('2d');\n      if (!ctx) return;\n      const createChart = (labels, data, label, backgroundColors) => {\n        this.chart = new Chart(ctx, {\n          type: 'bar',\n          data: {\n            labels,\n            datasets: [{\n              label,\n              data,\n              backgroundColor: backgroundColors,\n              borderWidth: 1\n            }]\n          },\n          options: {\n            responsive: true,\n            scales: {\n              y: {\n                beginAtZero: true\n              }\n            }\n          }\n        });\n      };\n      if (this.splitBySex) {\n        const maleUsers = this.users.filter(user => user.implementar.sex === 'M');\n        const femaleUsers = this.users.filter(user => user.implementar.sex === 'F');\n        const totalMaleAge = maleUsers.reduce((sum, user) => sum + parseInt(user.implementar.age), 0);\n        const totalFemaleAge = femaleUsers.reduce((sum, user) => sum + parseInt(user.implementar.age), 0);\n        const averageMaleAge = maleUsers.length ? totalMaleAge / maleUsers.length : 0;\n        const averageFemaleAge = femaleUsers.length ? totalFemaleAge / femaleUsers.length : 0;\n        createChart(['Masculino', 'Feminino'], [averageMaleAge, averageFemaleAge], 'Idade Média dos Usuários por Sexo', ['#007bff', '#dc3545']);\n      } else {\n        const totalAge = this.users.reduce((sum, user) => sum + parseInt(user.implementar.age), 0);\n        const averageAge = totalAge / this.users.length;\n        createChart(['Idade Média'], [averageAge], 'Idade Média dos Usuários', ['#007bff']);\n      }\n    }\n  }\n});","map":{"version":3,"names":["defineComponent","Chart","name","props","users","type","Array","required","splitBySex","Boolean","data","chart","mounted","$nextTick","renderChart","watch","updateChart","methods","destroy","ctx","$refs","ageChart","getContext","createChart","labels","label","backgroundColors","datasets","backgroundColor","borderWidth","options","responsive","scales","y","beginAtZero","maleUsers","filter","user","implementar","sex","femaleUsers","totalMaleAge","reduce","sum","parseInt","age","totalFemaleAge","averageMaleAge","length","averageFemaleAge","totalAge","averageAge"],"sources":["C:\\Users\\Usuário\\Documents\\teste-pratico\\front-end-teste\\src\\components\\chards\\AgeChard.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <canvas ref=\"ageChart\"></canvas>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import { defineComponent } from 'vue';\r\n  import Chart from 'chart.js/auto';\r\n  \r\n  export default defineComponent({\r\n    name: 'AgeChart',\r\n    props: {\r\n      users: {\r\n        type: Array,\r\n        required: true,\r\n      },\r\n      splitBySex: {\r\n        type: Boolean,\r\n        required: true,\r\n      },\r\n    },\r\n    data() {\r\n      return {\r\n        chart: null,\r\n      };\r\n    },\r\n    mounted() {\r\n      this.$nextTick(() => {\r\n        this.renderChart();\r\n      });\r\n    },\r\n    watch: {\r\n      users() {\r\n        this.updateChart();\r\n      },\r\n      splitBySex() {\r\n        this.updateChart();\r\n      },\r\n 