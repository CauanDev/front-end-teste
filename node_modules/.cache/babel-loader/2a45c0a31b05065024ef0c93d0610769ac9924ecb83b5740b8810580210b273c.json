.length;\n        this.chart = new Chart(ctx, {\n          type: 'bar',\n          data: {\n            labels: ['Idade Média'],\n            datasets: [{\n              label: 'Idade Média dos Usuários',\n              data: [averageAge],\n              backgroundColor: ['#007bff'],\n              borderWidth: 1\n            }]\n          },\n          options: {\n            responsive: true,\n            scales: {\n              y: {\n                beginAtZero: true\n              }\n            }\n          }\n        });\n      }\n    }\n  }\n});","map":{"version":3,"names":["defineComponent","Chart","name","props","users","type","Array","required","splitBySex","Boolean","data","chart","mounted","renderChart","watch","destroy","methods","ctx","$refs","ageChart","getContext","maleUsers","filter","user","sex","femaleUsers","totalMaleAge","reduce","sum","age","totalFemaleAge","averageMaleAge","length","averageFemaleAge","labels","datasets","label","backgroundColor","borderWidth","options","responsive","scales","y","beginAtZero","totalAge","averageAge"],"sources":["C:\\Users\\Usuário\\Documents\\teste-pratico\\front-end-teste\\src\\components\\chards\\AgeChard.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <canvas ref=\"ageChart\"></canvas>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import { defineComponent } from 'vue';\r\n  import Chart from 'chart.js/auto';\r\n  \r\n  export default defineComponent({\r\n    name: 'AgeChard',\r\n    props: {\r\n      users: {\r\n        type: Array,\r\n        required: true,\r\n      },\r\n      splitBySex: {\r\n        type: Boolean,\r\n        required: true,\r\n      },\r\n    },\r\n    data() {\r\n      return {\r\n        chart: null,\r\n      };\r\n    },\r\n    mounted() {\r\n      this.renderChart();\r\n    },\r\n    watch: {\r\n      users() {\r\n        if (this.chart) {\r\n          this.chart.destroy();\r\n        }\r\n        this.renderChart();\r\n      },\r\n      splitBySex() {\r\n        if (this.chart) {\r\n          this.chart.destroy();\r\n        }\r\n        this.renderChart();\r\n      }\r\n    },\r\n    methods: {\r\n      renderChart() {\r\n        const ctx = this.$refs.ageChart.getContext('2d');\r\n  \r\n        if (this.splitBySex) {\r\n          const maleUsers = this.users.filter(user => user.sex === 'M');\r\n          const femaleUsers = this.users.filter(user => user.sex === 'F');\r\n  \r\n          const totalMaleAge = maleUsers.reduce((sum, user) => sum + user.age, 0);\r\n          const totalFemaleAge = femaleUsers.reduce((sum, user) => sum + user.age, 0);\r\n  \r\n          const averageMaleAge = maleUsers.length ? totalMaleAge / maleUsers.length : 0;\r\n          const averageFemaleAge = femaleUsers.length ? totalFemaleAge / femaleUsers.length : 0;\r\n  \r\n          this.chart = new Chart(ctx, {\r\n            type: 'bar',\r\n            data: {\r\n              labels: ['Masculino', 'Feminino'],\r\n              datasets: [{\r\n                label: 'Idade Média dos Usuários por Sexo',\r\n                data: [averageMaleAge, averageFemaleAge],\r\n                backgroundColor: ['#007bff', '#dc3545'],\r\n                borderWidth: 1,\r\n              }],\r\n            },\r\n            options: {\r\n              responsive: true,\r\n              scales: {\r\n                y: {\r\n                  beginAtZero: true,\r\n                },\r\n              },\r\n            },\r\n          });\r\n        } else {\r\n          const totalAge = this.users.reduce((sum, user) => sum + user.age, 0);\r\n          const averageAge = totalAge / this.users.length;\r\n  \r\n          this.chart = new Chart(ctx, {\r\n            type: 'bar',\r\n            data: {\r\n              labels: ['Idade Média'],\r\n              datasets: [{\r\n                label: 'Idade Média dos Usuários',\r\n                data: [averageAge],\r\n                backgroundColor: ['#007bff'],\r\n                borderWidth: 1,\r\n              }]