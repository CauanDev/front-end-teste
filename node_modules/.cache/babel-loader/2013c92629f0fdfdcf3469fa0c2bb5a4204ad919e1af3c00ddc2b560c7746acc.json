{"ast":null,"code":"import http from '@/services/http.js';\nimport TitleView from '../components/Title/TitleView.vue';\nimport TableView from '../components/Table/TableView.vue';\nimport { format } from 'date-fns';\nexport default {\n  name: 'UpdateUser',\n  components: {\n    TitleView,\n    TableView\n  },\n  data() {\n    return {\n      users: [],\n      openModal: false,\n      newUser: {},\n      modalContent: '',\n      user: {\n        name: '',\n        password: '',\n        email: ''\n      }\n    };\n  },\n  methods: {\n    async getUsers() {\n      try {\n        const data = await http.get('/users');\n        this.users = data.data.users.map(user => {\n          return {\n            name: user.name,\n            email: user.email,\n            created_at: format(new Date(user.created_at), 'dd/MM/yyyy'),\n            implementar: {\n              name: user.name,\n              email: user.email,\n              id: user.id,\n              password: user.password\n            }\n          };\n        });\n      } catch (error) {\n        console.log(error.response);\n      }\n    },\n    showDetails(details) {\n      console.log(details);\n      this.modalContent = details;\n      this.newUser = details;\n      this.openModal = false;\n    },\n    async atualizar() {\n      if (this.user.name != '' || this.user.email != '' || this.user.password != '') {\n        if (this.user.name != '') {\n          this.newUser.name = this.user.name;\n        }\n        if (this.user.email != '') {\n          this.newUser.email = this.user.email;\n        }\n        if (this.user.password != '') {\n          this.newUser.password = this.user.password;\n        }\n        try {\n          console.log(this.newUser);\n          const data = await http.put('/users/' + this.modalContent.id, this.newUser);\n          console.log(data);\n        } catch (error) {\n          console.log(error.response);\n        }\n      }\n    }\n  },\n  mounted() {\n    this.getUsers();\n  }\n};","map":{"version":3,"names":["http","TitleView","TableView","format","name","components","data","users","openModal","newUser","modalContent","user","password","email","methods","getUsers","get","map","created_at","Date","implementar","id","error","console","log","response","showDetails","details","atualizar","put","mounted"],"sources":["C:\\Users\\Usuário\\Documents\\projeto_diagnostico\\client\\src\\views\\UpdateUser.vue"],"sourcesContent":["<template>\r\n  <div>\r\n  <TitleView title=\"Atualizar Usuários\"/>\r\n  </div>\r\n  <div class=\" flex justify-center mt-10\">\r\n     <TableView :headers=\"['Nome','Email','Criado Em','Ver Detalhes']\" :body=\"users\" @showDetails=\"showDetails\" class=\"w-[50%]\"/>  \r\n  </div>\r\n\r\n  <div id=\"default-modal\" :class=\"{'hidden':openModal}\" class=\" fixed inset-0 flex items-center justify-center bg-black bg-opacity-50\">\r\n    <div class=\"relative p-4 w-full max-w-2xl max-h-full\">\r\n        <!-- Modal content -->\r\n        <div class=\"relative bg-white rounded-lg shadow\">\r\n            <!-- Modal header -->\r\n            <div class=\"flex items-center justify-between p-4 md:p-5 border-b rounded-t\">\r\n                <h3 class=\"text-xl font-semibold text-gray-900 \">\r\n                    Alterar {{modalContent.name}}\r\n                </h3>\r\n                <button type=\"button\" class=\"text-gray-400 bg-transparent hover:bg-gray-200 hover:text-black-200 rounded-lg text-sm w-8 h-8 ms-auto inline-flex justify-center items-center \" data-modal-hide=\"default-modal\" @click=\"openModal=!openModal\">\r\n                    <svg class=\"w-3 h-3\" aria-hidden=\"true\" xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 14 14\">\r\n                        <path stroke=\"currentColor\" stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"m1 1 6 6m0 0 6 6M7 7l6-6M7 7l-6 6\"/>\r\n                    </svg>\r\n                    <span class=\"sr-only\">Close modal</span>\r\n                </button>\r\n            </div>\r\n            <!-- Modal body -->\r\n            <div class=\"p-4 md:p-5 space-y-4\">\r\n              <div>\r\n                <label  class=\"block mb-2 text-sm font-medium text-gray-900 \">Alterar Nome</label>\r\n                <input v-model=\"user.name\" type=\"text\"  class=\"bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5\" placeholder=\"Digite o novo nome\"  />\r\n              </div>\r\n              <div>\r\n                <label  class=\"block mb-2 text-sm font-medium text-gray-900 \">Alterar Senha</label>\r\n                <input v-model=\"user.password\" type=\"text\"  class=\"bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 \" placeholder=\"Digite a nova senha\"  />\r\n              </div>\r\n              <div>\r\n                <label  class=\"block mb-2 text-sm font-medium text-gray-900 \">Alterar Email</label>\r\n                <input v-model=\"user.email\" type=\"text\" class=\"bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 \" placeholder=\"Digite o novo email\"  />\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"flex items-center justify-center p-2 border-t border-gray-200 rounded-b\">\r\n              <button type=\"button\" class=\"focus:outline-none text-white bg-green-700 hover:bg-green-800 focus:ring-4 focus:ring-green-300 font-medium rounded-lg text-sm px-5 py-2.5 me-2 mb-2 dark:bg-green-600 dark:hover:bg-green-700 dark:focus:ring-green-800\" @click=\"atualizar\">Gravar</button>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\r\n\r\n\r\n\r\n</template>\r\n\r\n<script>\r\n\r\nimport http from '@/services/http.js';\r\nimport TitleView from '../components/Title/TitleView.vue';\r\nimport TableView from '../components/Table/TableView.vue';\r\nimport { format } from 'date-fns'; \r\n\r\nexport default {\r\n  name: 'UpdateUser',\r\n  components: {\r\n   TitleView,TableView\r\n  },\r\n  data() {\r\n    return {\r\n      users: [] ,\r\n      openModal: false,\r\n      newUser:{},\r\n      modalContent:'',\r\n      user: {\r\n        name: '',\r\n        password:'',\r\n        email:''        \r\n      }\r\n\r\n    };\r\n  },\r\n  methods: {\r\n  async getUsers(){\r\n    try {\r\n      const data = await http.get('/users');\r\n      this.users = data.data.users.map(user => {\r\n          return {\r\n            name: user.name,\r\n            email: user.email,      \r\n            created_at: format(new Date(user.created_at), 'dd/MM/yyyy'),\r\n            implementar: {\r\n              name: user.name,\r\n              email: user.email,     \r\n              id: user.id,\r\n              password: user.password\r\n            }\r\n\r\n          };\r\n        }\r\n\r\n      );\r\n\r\n    } catch (error) {\r\n      console.log(error.response)\r\n    }\r\n    },\r\n    showDetails(details) {\r\n      console.log(details)\r\n      this.modalContent = details;\r\n       this.newUser = details\r\n      this.openModal = false;\r\n    },\r\n    async atualizar(){\r\n\r\n      if((this.user.name!='' || this.user.email!='' || this.user.password!=''))\r\n      {\r\n        if(this.user.name!='')\r\n        {\r\n          this.newUser.name = this.user.name;\r\n        }  \r\n        if(this.user.email!='')\r\n        {\r\n          this.newUser.email = this.user.email;\r\n        }  \r\n        if(this.user.password!='')\r\n        {\r\n          this.newUser.password = this.user.password;\r\n        }  \r\n        try {\r\n          console.log(this.newUser)\r\n          const data = await http.put('/users/'+this.modalContent.id,this.newUser);\r\n          console.log(data)\r\n        } catch (error) {\r\n          console.log(error.response)\r\n        }\r\n        \r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getUsers();\r\n  }\r\n};\r\n</script>\r\n\r\n"],"mappings":"AAqDA,OAAOA,IAAG,MAAO,oBAAoB;AACrC,OAAOC,SAAQ,MAAO,mCAAmC;AACzD,OAAOC,SAAQ,MAAO,mCAAmC;AACzD,SAASC,MAAK,QAAS,UAAU;AAEjC,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACXJ,SAAS;IAACC;EACX,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAC;MACRC,SAAS,EAAE,KAAK;MAChBC,OAAO,EAAC,CAAC,CAAC;MACVC,YAAY,EAAC,EAAE;MACfC,IAAI,EAAE;QACJP,IAAI,EAAE,EAAE;QACRQ,QAAQ,EAAC,EAAE;QACXC,KAAK,EAAC;MACR;IAEF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACT,MAAMC,QAAQA,CAAA,EAAE;MACd,IAAI;QACF,MAAMT,IAAG,GAAI,MAAMN,IAAI,CAACgB,GAAG,CAAC,QAAQ,CAAC;QACrC,IAAI,CAACT,KAAI,GAAID,IAAI,CAACA,IAAI,CAACC,KAAK,CAACU,GAAG,CAACN,IAAG,IAAK;UACrC,OAAO;YACLP,IAAI,EAAEO,IAAI,CAACP,IAAI;YACfS,KAAK,EAAEF,IAAI,CAACE,KAAK;YACjBK,UAAU,EAAEf,MAAM,CAAC,IAAIgB,IAAI,CAACR,IAAI,CAACO,UAAU,CAAC,EAAE,YAAY,CAAC;YAC3DE,WAAW,EAAE;cACXhB,IAAI,EAAEO,IAAI,CAACP,IAAI;cACfS,KAAK,EAAEF,IAAI,CAACE,KAAK;cACjBQ,EAAE,EAAEV,IAAI,CAACU,EAAE;cACXT,QAAQ,EAAED,IAAI,CAACC;YACjB;UAEF,CAAC;QACH,CAEF,CAAC;MAEH,EAAE,OAAOU,KAAK,EAAE;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAACG,QAAQ;MAC5B;IACA,CAAC;IACDC,WAAWA,CAACC,OAAO,EAAE;MACnBJ,OAAO,CAACC,GAAG,CAACG,OAAO;MACnB,IAAI,CAACjB,YAAW,GAAIiB,OAAO;MAC1B,IAAI,CAAClB,OAAM,GAAIkB,OAAM;MACtB,IAAI,CAACnB,SAAQ,GAAI,KAAK;IACxB,CAAC;IACD,MAAMoB,SAASA,CAAA,EAAE;MAEf,IAAI,IAAI,CAACjB,IAAI,CAACP,IAAI,IAAE,EAAC,IAAK,IAAI,CAACO,IAAI,CAACE,KAAK,IAAE,EAAC,IAAK,IAAI,CAACF,IAAI,CAACC,QAAQ,IAAE,EAAE,EACvE;QACE,IAAG,IAAI,CAACD,IAAI,CAACP,IAAI,IAAE,EAAE,EACrB;UACE,IAAI,CAACK,OAAO,CAACL,IAAG,GAAI,IAAI,CAACO,IAAI,CAACP,IAAI;QACpC;QACA,IAAG,IAAI,CAACO,IAAI,CAACE,KAAK,IAAE,EAAE,EACtB;UACE,IAAI,CAACJ,OAAO,CAACI,KAAI,GAAI,IAAI,CAACF,IAAI,CAACE,KAAK;QACtC;QACA,IAAG,IAAI,CAACF,IAAI,CAACC,QAAQ,IAAE,EAAE,EACzB;UACE,IAAI,CAACH,OAAO,CAACG,QAAO,GAAI,IAAI,CAACD,IAAI,CAACC,QAAQ;QAC5C;QACA,IAAI;UACFW,OAAO,CAACC,GAAG,CAAC,IAAI,CAACf,OAAO;UACxB,MAAMH,IAAG,GAAI,MAAMN,IAAI,CAAC6B,GAAG,CAAC,SAAS,GAAC,IAAI,CAACnB,YAAY,CAACW,EAAE,EAAC,IAAI,CAACZ,OAAO,