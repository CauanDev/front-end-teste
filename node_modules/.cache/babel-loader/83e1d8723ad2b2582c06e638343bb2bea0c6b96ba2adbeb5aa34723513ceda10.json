ordValid=  this.user.password.length < 6 ? false : true\r\n      },\r\n      async getResults(){\r\n        try {\r\n                  const results = await http.get(\"/template\");\r\n                  results.data.users.map((item)=>{\r\n                    this.types.push(item.name)\r\n                  })\r\n                  alert(\"Resultado Criado com sucesso\")\r\n                  window.location.reload();\r\n        } catch (error) {\r\n          console.log(error)\r\n        }\r\n\r\n      }\r\n\r\n    },\r\n    mounted() {\r\n    this.getResults();\r\n  }\r\n  };\r\n  </script>\r\n\r\n  "],"mappings":";AA4GA,OAAOA,IAAG,MAAO,oBAAoB;AAEnC,eAAe;EACbC,IAAI,EAAC,eAAe;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,eAAe,EAAC,CAAC,SAAS,EAAC,SAAS,EAAC,SAAS,EAAC,SAAS,EAAC,SAAS,CAAC;MACnEC,cAAc,EAAC,CAAC,SAAS,EAAC,SAAS,EAAC,SAAS,EAAC,SAAS,EAAC,SAAS,CAAC;MAClEC,eAAe,EAAC,CAAC,SAAS,EAAC,SAAS,EAAC,SAAS,EAAC,UAAU,EAAC,UAAU,EAAC,WAAW,CAAC;MACjFC,KAAK,EAAC,EAAE;MACRC,eAAe,EAAC,EAAE;MAClBC,IAAI,EAAC;QACLC,YAAY,EAAC,EAAE;QACfC,cAAc,EAAE,EAAE;QAClBC,gBAAgB,EAAE,EAAE;QACpBC,aAAa,EAAE,EAAE;QACjBT,eAAe,EAAE,SAAS;QAC1BC,cAAc,EAAE,SAAS;QACzBC,eAAe,EAAE;MACnB;IAEA,CAAC;EACH,CAAC;EACDQ,OAAO,EAAE;IACP,MAAMC,UAAUA,CAAA,EAAG;MACjB,IAAI,CAACC,WAAU,GAAG,IAAI,CAACP,IAAI,CAACC,YAAW,KAAM,EAAC,IAC7B,IAAI,CAACD,IAAI,CAACE,cAAa,KAAM,EAAC,IAC9B,IAAI,CAACF,IAAI,CAACG,gBAAe,KAAM,EAAC,IAChC,IAAI,CAACH,IAAI,CAACI,aAAY,KAAM,EAAE;MAC/C,IAAG,IAAI,CAACG,WAAW,EAAC;QAElB,IAAI;UAGA,MAAM;YAACb;UAAI,IAAI,MAAMF,IAAI,CAACgB,IAAI,CAAC,UAAU,EAAC;YACxC,cAAc,EAAC,IAAI,CAACT,eAAe;YACnC,MAAM,EAAC,IAAI,CAACC,IAAI,CAACC,YAAY;YAC7B,QAAQ,EAAGQ,IAAI,CAACC,SAAS,CAAC,IAAI,CAACV,IAAI;UACrC,CAAC,CAAC;UACNW,OAAO,CAACC,GAAG,CAAClB,IAAI;QAChB,EAAE,OAAOmB,KAAK,EAAE;UACdF,OAAO,CAACC,GAAG,CAACC,KAAK,CAACC,QAAQ;QAC5B;MAEF;IACF,CAAC;IACDC,aAAaA,CAAA,EACb;MACE,MAAMC,YAAW,GAAI,4BAA4B;MACjD,IAAI,CAACC,YAAW,GAAI,CAACD,YAAY,CAACE,IAAI,CAAC,IAAI,CAACC,IAAI,CAACC,KAAK,CAAC,GAAE,KAAK,GAAC,IAAI;IACrE,CAAC;IACDC,gBAAgBA,CAAA,EAChB;MACE,IAAI,CAACC,e