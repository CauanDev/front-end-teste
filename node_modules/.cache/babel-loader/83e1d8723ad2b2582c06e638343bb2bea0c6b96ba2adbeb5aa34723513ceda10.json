ror);\n      }\n    }\n  },\n  mounted() {\n    this.getResults();\n  }\n};","map":{"version":3,"names":["http","name","data","salarioOperador","salarioGerente","salarioLoterico","types","tipoDiagnostico","body","nomeLoterica","tempoFecharTFL","tempoLancarCaixa","tempoConferir","methods","submitForm","isFormValid","post","JSON","stringify","console","log","error","response","validateEmail","emailPattern","isEmailValid","test","user","email","validatePassword","isPasswordValid","password","length","getResults","results","get","users","map","item","push","alert","window","location","reload","mounted"],"sources":["C:\\Users\\Usuário\\Documents\\projeto_diagnostico\\client\\src\\views\\CreateResults.vue"],"sourcesContent":["<template>\r\n  <div class=\"flex h-screen\">\r\n    <div class=\"flex-1\">\r\n      <div class=\"flex items-center justify-center h-full bg-gray-100\">\r\n        <div class=\"max-w-md w-full\">\r\n          <form @submit.prevent=\"submitForm\" class=\"bg-white shadow-md rounded px-8 pt-6 pb-8 mb-4\">\r\n            <div class=\"mb-4\">\r\n              <label class=\"block text-gray-700 text-sm font-bold mb-2\" for=\"name\">\r\n                Nome da Lotérica\r\n              </label>\r\n              <input v-model=\"body.nomeLoterica\"\r\n              class=\"shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline\" \r\n              type=\"text\" \r\n              placeholder=\"Digite o nome da Lotérica\">\r\n            </div>\r\n            <div class=\"mb-4\">\r\n              <label class=\"block text-gray-700 text-sm font-bold mb-2\" for=\"email\">\r\n                Tempo para Fechar um TFL\r\n              </label>\r\n              <input v-model=\"body.tempoFecharTFL\"\r\n              class=\"shadow appearance-none border rounded w-full py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline\" \r\n              type=\"text\" \r\n              plac