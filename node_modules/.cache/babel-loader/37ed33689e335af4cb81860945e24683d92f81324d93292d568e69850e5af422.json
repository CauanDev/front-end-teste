{"ast":null,"code":"import { defineComponent } from 'vue';\nimport { Chart, registerables } from 'chart.js';\nimport { Bar, mixins } from 'chart.js';\nChart.register(...registerables);\nexport default defineComponent({\n  name: 'SexChard',\n  extends: Bar,\n  props: {\n    users: {\n      type: Array,\n      required: true\n    }\n  },\n  mixins: [mixins.reactiveData],\n  mounted() {\n    this.renderChart({}, {\n      reactive: true\n    });\n  },\n  watch: {\n    users: {\n      handler() {\n        this.renderChart(this.chartData, {\n          reactive: true\n        });\n      },\n      deep: true\n    }\n  },\n  computed: {\n    chartData() {\n      const maleCount = this.users.filter(user => user.sex === 'M').length;\n      const femaleCount = this.users.filter(user => user.sex === 'F').length;\n      return {\n        labels: ['Masculino', 'Feminino'],\n        datasets: [{\n          label: 'Número de Usuários por Sexo',\n          data: [maleCount, femaleCount],\n          backgroundColor: ['#007bff', '#dc3545'],\n          borderWidth: 1\n        }]\n      };\n    }\n  }\n});","map":{"version":3,"names":["defineComponent","Chart","registerables","Bar","mixins","register","name","extends","props","users","type","Array","required","reactiveData","mounted","renderChart","reactive","watch","handler","chartData","deep","computed","maleCount","filter","user","sex","length","femaleCount","labels","datasets","label","data","backgroundColor","borderWidth"],"sources":["C:\\Users\\Usuário\\Documents\\teste-pratico\\front-end-teste\\src\\components\\chards\\SexChard.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <canvas ref=\"sexChart\"></canvas>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import { defineComponent } from 'vue';\r\n  import { Chart, registerables } from 'chart.js';\r\n\r\n  import { Bar, mixins } from 'chart.js';\r\n  \r\n  Chart.register(...registerables);\r\n  \r\n  export default defineComponent({\r\n    name: 'SexChard',\r\n    extends: Bar,\r\n    props: {\r\n      users: {\r\n        type: Array,\r\n        required: true,\r\n      },\r\n    },\r\n    mixins: [mixins.reactiveData],\r\n    mounted() {\r\n      this.renderChart({}, { reactive: true });\r\n    },\r\n    watch: {\r\n      users: {\r\n        handler() {\r\n          this.renderChart(this.chartData, { reactive: true });\r\n        },\r\n        deep: true,\r\n      },\r\n    },\r\n    computed: {\r\n      chartData() {\r\n        const maleCount = this.users.filter(user => user.sex === 'M').length;\r\n        const femaleCount = this.users.filter(user => user.sex === 'F').length;\r\n  \r\n        return {\r\n          labels: ['Masculino', 'Feminino'],\r\n          datasets: [{\r\n            label: 'Número de Usuários por Sexo',\r\n            data: [maleCount, femaleCount],\r\n            backgroundColor: ['#007bff', '#dc3545'],\r\n            borderWidth: 1,\r\n          }],\r\n        };\r\n      },\r\n    },\r\n  });\r\n  </script>\r\n  "],"mappings":"AAOE,SAASA,eAAc,QAAS,KAAK;AACrC,SAASC,KAAK,EAAEC,aAAY,QAAS,UAAU;AAE/C,SAASC,GAAG,EAAEC,MAAK,QAAS,UAAU;AAEtCH,KAAK,CAACI,QAAQ,CAAC,GAAGH,aAAa,CAAC;AAEhC,eAAeF,eAAe,CAAC;EAC7BM,IAAI,EAAE,UAAU;EAChBC,OAAO,EAAEJ,GAAG;EACZK,KAAK,EAAE;IACLC,KAAK,EAAE;MACLC,IAAI,EAAEC,KAAK;MACXC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDR,MAAM,EAAE,CAACA,MAAM,CAACS,YAAY,CAAC;EAC7BC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,WAAW,CAAC,CAAC,CAAC,EAAE;MAAEC,QAAQ,EAAE;IAAK,CAAC,CAAC;EAC1C,CAAC;EACDC,KAAK,EAAE;IACLR,KAAK,EAAE;MACLS,OAAOA,CAAA,EAAG;QACR,IAAI,CAACH,WAAW,CAAC,IAAI,CAACI,SAAS,EAAE;UAAEH,QAAQ,EAAE;QAAK,CAAC,CAAC;MACtD,CAAC;MACDI,IAAI,EAAE;IACR;EACF,CAAC;EACDC,QAAQ,EAAE;IACRF,SAASA,CAAA,EAAG;MACV,MAAMG,SAAQ,GAAI,IAAI,CAACb,KAAK,CAACc,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACC,GAAE,KAAM,GAAG,CAAC,CAACC,MAAM;MACpE,MAAMC,WAAU,GAAI,IAAI,CAAClB,KAAK,CAACc,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACC,GAAE,KAAM,GAAG,CAAC,CAACC,MAAM;MAEtE,OAAO;QACLE,MAAM,EAAE,CAAC,WAAW,EAAE,UAAU,CAAC;QACjCC,QAAQ,EAAE,CAAC;UACTC,KAAK,EAAE,6BAA6B;UACpCC,IAAI,EA