{"ast":null,"code":"import http from '@/services/http.js';\nimport TitleView from '../components/Title/TitleView.vue';\nimport TableView from '../components/Table/TableView.vue';\nimport { format } from 'date-fns';\nexport default {\n  name: 'UpdateUser',\n  components: {\n    TitleView,\n    TableView\n  },\n  data() {\n    return {\n      users: []\n    };\n  },\n  methods: {\n    async getUsers() {\n      try {\n        const data = await http.get('/users');\n        console.log(data.data.users);\n        this.users = data.data.users.map(user => {\n          return {\n            name: user.name,\n            email: user.email,\n            created_at: format(new Date(user.created_at), 'dd/MM/yyyy')\n          };\n        });\n      } catch (error) {\n        console.log(error.response);\n      }\n    },\n    showDetails(details) {\n      this.modalContent = details;\n      this.showModal = true;\n    },\n    closeModal() {\n      this.showModal = false;\n      this.modalContent = '';\n    }\n  },\n  mounted() {\n    this.getUsers();\n  }\n};","map":{"version":3,"names":["http","TitleView","TableView","format","name","components","data","users","methods","getUsers","get","console","log","map","user","email","created_at","Date","error","response","showDetails","details","modalContent","showModal","closeModal","mounted"],"sources":["C:\\Users\\Usuário\\Documents\\projeto_diagnostico\\client\\src\\views\\UpdateUser.vue"],"sourcesContent":["<template>\r\n  <div>\r\n  <TitleView title=\"Atualizar Usuários\"/>\r\n  </div>\r\n  <div class=\" flex justify-center mt-10\">\r\n     <TableView :headers=\"['Nome','Email','Criado Em','Ver Detalhes','Teste']\" :body=\"users\" class=\"w-[50%]\"/>  \r\n\r\n  </div>\r\n  <div class=\"modal-content\">\r\n        <span class=\"close\" @click=\"closeModal\">&times;</span>\r\n        <p>{{ modalContent }}</p>\r\n      </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport http from '@/services/http.js';\r\nimport TitleView from '../components/Title/TitleView.vue';\r\nimport TableView from '../components/Table/TableView.vue';\r\nimport { format } from 'date-fns'; \r\n\r\nexport default {\r\n  name: 'UpdateUser',\r\n  components: {\r\n   TitleView,TableView\r\n  },\r\n  data() {\r\n    return {\r\n      users: [] \r\n    };\r\n  },\r\n  methods: {\r\n  async getUsers(){\r\n    try {\r\n      const data = await http.get('/users');\r\n      console.log(data.data.users)\r\n      this.users = data.data.users.map(user => {\r\n          return {\r\n            name: user.name,\r\n            email: user.email,      \r\n            created_at: format(new Date(user.created_at), 'dd/MM/yyyy') \r\n          };\r\n        }\r\n\r\n      );\r\n\r\n    } catch (error) {\r\n      console.log(error.response)\r\n    }\r\n    },\r\n    showDetails(details) {\r\n      this.modalContent = details;\r\n      this.showModal = true;\r\n    },\r\n    closeModal() {\r\n      this.showModal = false;\r\n      this.modalContent = '';\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getUsers();\r\n  }\r\n};\r\n</script>\r\n<style>\r\n/* Add some basic styling for the modal */\r\n.modal {\r\n  display: block;\r\n  position: fixed;\r\n  z-index: 1;\r\n  left: 0;\r\n  top: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  overflow: auto;\r\n  background-color: rgb(0,0,0);\r\n  background-color: rgba(0,0,0,0.4);\r\n}\r\n\r\n.modal-content {\r\n  background-color: #fefefe;\r\n  margin: 15% auto;\r\n  padding: 20px;\r\n  border: 1px solid #888;\r\n  width: 80%;\r\n}\r\n\r\n.close {\r\n  color: #aaa;\r\n  float: right;\r\n  font-size: 28px;\r\n  font-weight: bold;\r\n}\r\n\r\n.close:hover,\r\n.close:focus {\r\n  color: black;\r\n  text-decoration: none;\r\n  cursor: pointer;\r\n}\r\n</style>"],"mappings":"AAgBA,OAAOA,IAAG,MAAO,oBAAoB;AACrC,OAAOC,SAAQ,MAAO,mCAAmC;AACzD,OAAOC,SAAQ,MAAO,mCAAmC;AACzD,SAASC,MAAK,QAAS,UAAU;AAEjC,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACXJ,SAAS;IAACC;EACX,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE;IACT